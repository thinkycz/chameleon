    "description": "Elasticsearch implementation for Laravel Scout",
    "keywords": ["elasticsearch", "scout"],
    "license": "proprietary",
    "type": "library",
    "authors": [
        {
            "name": "Long Do",
            "email": "leo@nulisec.com",
            "homepage": "https://nulisec.com"
        }
    ],
    "autoload": {
